@model List<Model.EF.Project>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_CategoriesPartial.cshtml";
}


<div class="table-responsive">
    <table class="table table-striped table-bordered table-hover">
        <thead>
            <tr>

                <th>Tài khoản</th>
                <th>Tên</th>
                <th>Email</th>
                <th>Trạng thái</th>
                <th>#</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr id="row_@item.idProject">

                    <td>@item.projectName</td>
                    <td>@item.description</td>
                    <td><a href="#" class="btn-active" data-id="@item.idProject"></a></td>
                    <td>
                        <a href="/Admin/User/Edit/@item.idProject">Sửa</a>

                    </td>
                </tr>
            }


        </tbody>
    </table>

